{
  "name": "cdktf-project",
  "version": "1.0.0",
  "description": "CDK for Terraform project with best practices",
  "main": "lib/main.js",
  "scripts": {
    "get": "cdktf get",
    "build": "tsc",
    "watch": "tsc -w",
    "synth": "cdktf synth",
    "synth:prod": "cdktf synth --var-file=terraform.prod.tfvars",
    "synth:prod:quiet": "CI=1 NO_COLOR=1 CDKTF_LOG_LEVEL=error cdktf synth --var-file=terraform.prod.tfvars",
    "synth:dev": "cdktf synth --var-file=terraform.dev.tfvars",
    "plan": "cdktf diff",
    "plan:prod": "cdktf diff --var-file=terraform.prod.tfvars",
    "plan:dev": "cdktf diff --var-file=terraform.dev.tfvars",
    "deploy": "cdktf deploy",
    "deploy:prod": "cdktf deploy --var-file=terraform.prod.tfvars --auto-approve",
    "deploy:prod:quiet": "CI=1 NO_COLOR=1 CDKTF_LOG_LEVEL=error cdktf deploy --var-file=terraform.prod.tfvars --auto-approve",
    "deploy:dev": "cdktf deploy --var-file=terraform.dev.tfvars --auto-approve",
    "deploy:auto": "cdktf deploy -a",
    "destroy": "cdktf destroy --var-file=terraform.prod.tfvars",
    "output": "cdktf output",
    "deploy:full": "scripts/cdktf-reset-and-deploy.sh",
    "clean": "rm -rf cdktf.out terraform.*.tfstate* .terraform lib dist node_modules/.cache",
    "clean:state": "rm -rf cdktf.out terraform.*.tfstate* || true",
    "clean:cache": "rm -rf node_modules/.cache lib dist",
    "reset": "npm run clean && npm install && npm run get",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "pre-commit": "npm run lint && npm run format:check && npm test",
    "upgrade": "npm update @cdktf/provider-aws @cdktf/provider-cloudflare cdktf constructs"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "cdktf-cli": "0.21.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@cdktf/provider-aws": "21.9.1",
    "@cdktf/provider-cloudflare": "13.4.0",
    "@cdktf/provider-http": "^10.0.0",
    "cdktf": "0.21.0",
    "constructs": "^10.3.0"
  }
}
